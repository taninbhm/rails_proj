<h1>Home</h1>
<div class="container">
  <div class="row">
    <div class="col-md-8">
      <div class="left-container">
      <h4>List of foods</h4>
      <ul class="list-group list-group-flush ">
      <% @foods.each do |food| %>
        <li class="list-group-item"><%= link_to food.dish, food %></li>
      <% end %>
      </ul>
      </div>
    </div>
    <div class="col-md-3">
      <div class="right-container">
        Right Side
      </div>
    </div>
  </div>
</div>